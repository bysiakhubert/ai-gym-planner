---
/**
 * Edit page for new/unsaved plans generated by AI
 *
 * This page loads plan data from localStorage (key: "plan_to_edit")
 * and allows editing before saving as a new plan.
 */
import Layout from "@/layouts/Layout.astro";
import { PlanEditorView } from "@/components/plans/editor";

export const prerender = false;
---

<Layout title="GymPlanner - Edytuj nowy plan">
  <PlanEditorView client:load />
</Layout>
